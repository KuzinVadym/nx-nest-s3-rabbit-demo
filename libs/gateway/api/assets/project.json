{
  "name": "gateway-api-assets",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/gateway/api/assets/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/gateway/api/assets",
        "tsConfig": "libs/gateway/api/assets/tsconfig.lib.json",
        "packageJson": "libs/gateway/api/assets/package.json",
        "main": "libs/gateway/api/assets/src/index.ts",
        "assets": ["libs/gateway/api/assets/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs gateway-api-assets {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/gateway/api/assets/jest.config.ts"
      }
    }
  },
  "tags": ["lib"]
}
